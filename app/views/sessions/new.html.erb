<h1>Log in</h1>

<div id='signin_form'>
  <%= form_tag sessions_path do |f| %>

    <%= label_tag :email %>
    <%= text_field_tag :email %>
    <br>

    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>

    <%= submit_tag 'Log In' %>

  <% end %>
</div>

<p><%= link_to 'Forgot password?', new_password_reset_path %></p>
