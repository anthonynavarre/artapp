<h1><%= @space.name %></h1>
  
<br>

<div id='space_details'>
  <% if @space.url %>
    Website: <%= link_to @space.url, @space.url %>
  <% end %>
</div>

<br>

<% if @space.photos %>
  <div id='space_photos'>
    <% @space.photos.each do |photo| %>
      <h4><%= photo.name %></h4>
      <p><%= image_tag photo.image %></p>
    <% end %>
  </div>
<% end %>