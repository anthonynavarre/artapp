<% if current_user %>
  Logged in as <%= current_user.email %><br>
  <%= link_to 'Log Out', logout_path, method: :delete %>
<% else %>
  <%= link_to 'Sign Up', signup_path %> | 
  <%= link_to 'Log In', login_path %>
<% end %>

  <p>Registered Users:</p>
  <p>
<% @users.each do |user| %>
    <%= user.email %>
  </p>
<% end %>