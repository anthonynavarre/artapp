<% if current_user %>
  Logged in as <%= current_user.email %><br>
  <%= link_to 'Log Out', logout_path %>
<% else %>
  <%= link_to 'Sign Up', new_user_path %> | 
  <%= link_to 'Log In', login_path %>
<% end %>

<% @users.each do |user| %>
  <p>Registered Users:</p>
  <p>
    <%= user.email %>
  </p>
<% end %>